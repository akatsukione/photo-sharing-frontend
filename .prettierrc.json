{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "importOrder": [
    "@/(.*)",
    "<THIRD_PARTY_MODULES>",
    "^[./]",
    "^routes",
    "^pages",
    "^controllers",
    "^components",
    "^contexts",
    "^hooks",
    "^config",
    "^consts",
    "^utils"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
